<h1>Unsubscription List</h1>
<table class="table table-hover">
	<thead>
		<tr>
			<th>Unsubscriber ID</th>
			<th>Subscribed ID</th>
			<th> </th>
		</tr>
	</thead>

	<tbody>
		<% @messages.each do |message| %>
		<tr>
			<td><%= message.unsubscriber_id %></td>
			<td><%=	message.unsubscribed_id %></td>			
			<td>
				<%=button_to "Agree", messages_delete_path(:unsid1 => message.unsubscriber_id,  :unsid2 => message.unsubscribed_id), class: "btn btn-small btn-primary"  %>
			</td>
		<% end %>
	</tbody>
</table>

